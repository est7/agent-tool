{
  "mcpServers": {
    "claudecode-mcp-async": {
      "args": [
        "claudecode-mcp-async"
      ],
      "command": "uvx",
      "env": {}
    },
    "codex-mcp-async": {
      "args": [
        "codex-mcp-async"
      ],
      "command": "uvx",
      "env": {}
    },
    "exa-mcp": {
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.exa.ai/mcp"
      ],
      "command": "npx"
    },
    "gemini-cli-mcp-async": {
      "args": [
        "gemini-cli-mcp-async"
      ],
      "command": "uvx",
      "env": {}
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "type": "stdio"
    }
  },
  "contextFileName": ["AGENTS.md"]
}
